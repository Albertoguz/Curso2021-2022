@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vocab: <http://www.preventbicyleaccidents-app.es/group02/ontology/bicycletrafficaccident#> .
@base <http://example.com/ns#>.


<#TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "cruces_semaforizados-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://www.preventbicycleaccidents-app.es/group02/resources/Crossing/{id}";
    rr:class vocab:Crossing
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasAddress; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap2>;
	  rr:joinCondition [
        rr:child "descripcion"; rr:parent "descripcion";
      ]
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:crossIn; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap3>;
	  rr:joinCondition [
        rr:child "distrito"; rr:parent "distrito";
      ]
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasCoordinates; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap4>;
	  rr:joinCondition [
        rr:child "id"; rr:parent "id";
      ]
    ]
  ].
  
  
<#TriplesMap2> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "cruces_semaforizados-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://www.preventbicyleaccidents-app.es/group02/resources/Address/{descripcion}";
    rr:termType rr:IRI;
	rr:class vocab:Address
  ];
  
  rr:predicateObjectMap [
  rr:predicateMap [
      rr:constant vocab:hasAddressName; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "descripcion";
      rr:termType rr:Literal
    ]
  ].
  
  
<#TriplesMap3> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "cruces_semaforizados-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
rr:template "http://www.preventbicyleaccidents-app.es/group02/resources/District/{distrito}";
    rr:termType rr:IRI;
	rr:class vocab:District
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasCode; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap5>;
	  	rr:joinCondition [
        rr:child "distrito"; rr:parent "ID_DISTRICT";
      ]
    ]
  ].
  
 <#TriplesMap4> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "cruces_semaforizados-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://www.preventbicyleaccidents-app.es/group02/resources/Geometry/crossing{id}";
    rr:termType rr:IRI;
	rr:class vocab:Geocoder
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasLat; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "latitud"; rr:dataType <http://www.w3.org/2001/XMLSchema#float>
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasLong; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "longitud"; rr:dataType <http://www.w3.org/2001/XMLSchema#float>
    ]
  ].
  
<#TriplesMap5> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "distritos-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://www.preventbicyleaccidents-app.es/group02/resources/District/{ID_DISTRICT}";
    rr:termType rr:IRI;
    rr:class vocab:District
  ];

  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasCode; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "ID_DISTRICT"; rr:termtype rr:Literal
      ]
  ];

  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasDistrictName; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "NOMBRE"; rr:termtype rr:Literal
    ]
  ].