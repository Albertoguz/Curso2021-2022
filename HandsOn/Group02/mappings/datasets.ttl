@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vocab: <http://www.preventbicyleaccidents-app.es/group02/ontology/bicycletrafficaccident#> .
@base <http://example.com/ns#>.

<#TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "./data/AccidentesBicicletas_2021-updated.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://www.preventbicyleaccidents-app.es/group02/resources/Accident/{EXPEDIENTE}";
    rr:termType rr:IRI;
	rr:class vocab:Accident
  ];
  
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasPlace; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap2>;
	  rr:joinCondition [
        rr:child "CALLE"; rr:parent "CALLE";
      ]
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasVictim; rr:termType rr:IRI
	];
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap4>;
	  rr:joinCondition [
        rr:child "VICTIMA"; rr:parent "VICTIMA";
      ]
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
	  rr:constant vocab:hasDate; rr:termType rr:IRI
    ];
	rr:objectMap [
      rml:reference "DATETIME";
      rr:termType rr:Literal;
	  rr:dataType <http://www.w3.org/2001/XMLSchema#dateTime>
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
      rr:constant vocab:hasWeather; rr:termType rr:IRI
    ];
	rr:objectMap [
      rml:reference "TIEMPO";
      rr:termType rr:Literal
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicateMap [
      rr:constant vocab:typeBicycle; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "TIPOVEHICULO";
      rr:termType rr:Literal
    ]
  ];
  
  
  rr:predicateObjectMap [
    rr:predicateMap [
      rr:constant vocab:causedBy; rr:termType rr:IRI
	];
    rr:objectMap [
      rml:reference "TIPOACCIDENTE";
      rr:termType rr:Literal
    ]
  ].
  


